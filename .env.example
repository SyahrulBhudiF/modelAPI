# AI Model Serving API - Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# Authentication
# =============================================================================

# Static Bearer token for API authentication
# Generate a secure token: python -c "import secrets; print(secrets.token_urlsafe(32))"
AI_API_TOKEN=your_super_secret_token_here

# =============================================================================
# Model Registry
# =============================================================================

# Format: model_name:handler_key:model_dir
# Multiple models can be separated by comma
#
# Example single model:
#   MODEL_REGISTRY=tabr:tabr:models/tabr_experiment
#
# Example multiple models:
#   MODEL_REGISTRY=tabr:tabr:models/tabr_v1,tabr_v2:tabr:models/tabr_v2

MODEL_REGISTRY=tabr:tabr:tabr_experiment/before_flatten

# =============================================================================
# Server Configuration
# =============================================================================

# Host to bind the server to
HOST=0.0.0.0

# Port to listen on
PORT=8000

# Enable debug mode (set to true for development)
DEBUG=false
